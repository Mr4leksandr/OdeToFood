@model IEnumerable<OdeToFood.Models.RestorantReview>

@{
    ViewBag.Title = "Index";
}

<h2>The Latest Reviews</h2>

@foreach (var item in Model) {
    <div class="review">
        <h4>@item.Name</h4>
        <span>@item.Rating / 10</span>
        <p>@item.City, @item.Country</p>
    </div>
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>